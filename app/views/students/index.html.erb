<h1>List of Students</h1>
<div class="flickr_pagination">
<%= will_paginate @student %>
</div>
<table>
<thead>
<tr>
<th>Firstname</th>
<th>Roll</th>
<th colspan=3>Action</th>
</tr>
</thead>
<body>
   <% @student.each do |t|%>
<tr> <td><%=t.firstname%></td>
 <td><%=t.roll%></td>
 <td><%=link_to 'show',student_path(t.id)%></td>
 <% if logged_in? && current_user == t.course%>
 <td><%=link_to 'delete',student_path(t.id),method: :delete%></td>
 <td><%=link_to 'edit',edit_student_path(t.id)%></td></tr><br>
 <%end%> 
<%end%>

</body>
</table>
<p><%=link_to 'new',new_student_path%></p>
<div class="flickr_pagination">
<%= will_paginate @student %>
</div>